}
layer {
  name: "drop7-cla"
  type: "Dropout"
  bottom: "fc7-cla"
  top: "fc7-cla"
  dropout_param {
    dropout_ratio: 0.4
  }
}
layer {
  name: "fc8-cla"
  type: "InnerProduct"
  bottom: "fc7-cla"
  top: "fc8-cla"
  param {
    lr_mult: 1
    decay_mult: 2
  }
  param {
