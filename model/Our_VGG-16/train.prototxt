layer {
  name: "accuracy"
  type: "Accuracy"
  bottom: "fc8-cla"
  bottom: "label"
  top: "accuracy"
  include {
    phase: TEST
  }
}

layer {
  name: "accuracy_top5"
  type: "Accuracy"
  bottom: "fc8-cla"
  bottom: "label"
  top: "accuracy_top5"
  accuracy_param {
   top_k: 5
  }
  include {
    phase: TEST
  }
}

layer {
  name: "loss"
  type: "SoftmaxWithLoss"
  bottom: "fc8-cla"
  bottom: "label"
  top: "loss"
}

